<Ui>
  <Frame parent="UIParent">
    <Size x="50" y="50"/>
    <Anchors>
      <Anchor point="CENTER"/>
    </Anchors>
    <Frames>
      <EditBox parentKey="Edit" autofocus="false" inherits="InputBoxTemplate">
        <Size x="250" y="50"/>
        <Anchors>
          <Anchor point="CENTER"/>
        </Anchors>
        <Scripts>
          <OnKeyDown>
            self:SetPropagateKeyboardInput(key == "ENTER")
          </OnKeyDown>
        </Scripts>
      </EditBox>
      <Button text="test" inherits="SecureActionButtonTemplate,UIPanelDynamicResizeButtonTemplate">
        <Anchors>
          <Anchor point="TOPLEFT" relativeKey="$parent.Edit" y="30"/>
        </Anchors>
      </Button>
    </Frames>
  </Frame>
</Ui>
